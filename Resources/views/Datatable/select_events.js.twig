{% if view_select_events.select %}
    {% if (view_select_events.select['vars'] is defined) %}
        {% set vars = view_select_events.select['vars'] %}
    {% else %}
        {% set vars = {} %}
    {% endif %}
    .on('select', {% include view_select_events.select['template'] with vars %})
{% endif %}
{% if view_select_events.deselect %}
    {% if (view_select_events.deselect['vars'] is defined) %}
        {% set vars = view_select_events.deselect['vars'] %}
    {% else %}
        {% set vars = {} %}
    {% endif %}
    .on('deselect', {% include view_select_events.deselect['template'] with vars %})
{% endif %}
