{
    "table": selector,
    "ajax":  {
        "url": "{{ view_editor.ajaxUrl }}",
        "type": "{{ view_editor.ajaxType }}",
        {% if view_editor.ajaxData %}"data": {{ include(view_editor.ajaxData)|raw  }}{% endif %}
    },
    "display": "{{ view_editor.display }}",
    "idSrc": "{{ view_options.rowId }}",
    {% if view_editor.templateFormId %}"template": "{{ view_editor.templateFormId }}",{% endif %}
    "fields": [
        {% for column in view_columns if column.editable is defined and column.editable %}
            {
                className: "{{ column.editorOptions.className }}",
                data: "{{ column.editorOptions.data }}", //todo, can be string,int, func
                def: "{{ column.editorOptions.def }}", //todo, can be string,int, func
                entityDecode: {{ column.editorOptions.entityDecode }},
                label: "{{ column.editorOptions.label }}",
                name: "{{ column.editorOptions.name }}",
                type: "{{  column.editorOptions.type }}",
                options: {{ column.editorOptions.options|json_encode|raw }},
                attr: {{ column.editorOptions.attr|json_encode|raw }},
                labelInfo: "{{  column.editorOptions.labelInfo }}",
                opts: {{ column.editorOptions.opts|json_encode|raw }}
            },
        {% endfor %}
    ],
}
