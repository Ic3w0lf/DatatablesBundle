{
    "table": selector,
    "ajax":  {
        "url": "{{ view_editor.ajaxUrl }}",
        "type": "{{ view_editor.ajaxType }}",
        {% if view_editor.ajaxData is not null %}"data": {{ include(view_editor.ajaxData)|raw  }}{% endif %}
    },
    "display": "{{ view_editor.display }}",
    "idSrc": "{{ view_options.rowId }}",
    "fields": [
        {% for column in view_columns %}
            { label: "{{ column.title }}", name: "{{ column.data }}" },
        {% endfor %}
    ],
}
